#include <stdio.h>
#include <stdlib.h>

int comparador(float x, float y){
    if( x > y ){
        return 1;
    } else{
        return 0;
    }
}

void myqsort(float *vetor, int n, int (*c) (float,float)){
    int i,j;
    float aux;

    //Ordenação
    for(i=0;i<n;i++){
        for(j=i+1;j<n;j++){
            if((c(vetor[i],vetor[j])) == 1){
                aux = vetor[i];
                vetor[i] = vetor[j];
                vetor[j] = aux;
            }
        }
    }
}

int main(){
    int n,i;
    float *vetor;
    printf("Informe a quantidade de elementos \n");
    scanf("%d", &n);
    vetor = (float*) malloc(n*sizeof(float));
    printf("Informe os elementos \n");
    for(i=0;i<n;i++){
        scanf("%f", &vetor[i]);
    }
    printf("\n");

    myqsort(vetor,n,comparador);

    //Mostra o vetor ordenado
    for(i=0;i<n;i++){
        printf("%f \n", vetor[i]);
    }

    free(vetor);

    return 0;
}
