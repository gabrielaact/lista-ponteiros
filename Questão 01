#include <stdio.h>

void main() {
    int i = 3, j = 5;
    int *p, *q;

    p = &i;
    q = &j;

    if(p == &i) {
        printf("Resultado da primeira expressao: Verdadeiro\n");
    }
    else {
        printf("Resultado da primeira expressao: Falso\n");
    }

    printf("Resultado da segunda expressao: %d\n", *p - *q);

    printf("Resultado da terceira expressao: %d\n", **&p);

    printf("Resultado da quarta expressao: %d\n", 3 - *p/(*q) + 7);

    /*
        Respostas:
        O resultado da primeira expressao é verdadeiro
        O resultado da segunda expressao é: -2
        O resultado da terceira expressao é: 3
        O resultado da quarta expressao é: 10
    */
}
